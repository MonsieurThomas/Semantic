datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int     @id @default(autoincrement())
  username String  @unique
  email    String  @unique
  password String
}

model Document {
  id       Int      @id @default(autoincrement())
  date     DateTime
  color    String
  title    String
  url      String
  theme    String[]
  page     Int
}
